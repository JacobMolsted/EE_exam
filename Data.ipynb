    "InstalledCap_df = InstalledCap_df[InstalledCap_df['2022 [MW]'] > 0]\n",
    "\n",
    "#Resetting the index\n",
    "InstalledCap_df = InstalledCap_df.reset_index(drop=True)\n",
    "\n",
    "#InstalledCap_df \n"
